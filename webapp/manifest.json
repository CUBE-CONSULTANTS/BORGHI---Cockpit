{
	"_version": "1.66.0",
	"sap.app": {
		"id": "sap.ui.demo.fiori2",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.demo.fiori2.view.App",
			"type": "XML",
			"async": true,
			"id": "fcl"
		},
		"dependencies": {
			"minUI5Version": "1.98.0",
			"libs": {
				"sap.f": {},
				"sap.m": {},
				"sap.ui.core": {},
				"sap.uxap": {},
				"sap.viz": {}

			}
		},
		"resources": {
			"css": [
			  {
				"uri": "css/style.css"
			  }
			],
      "img": [
        {
          "uri": "public/images/",
          "name": "borghi.images"
        }
      ]
		},
		"config": {
			"fullWidth": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "sap.ui.demo.fiori2.view",
				"controlId": "flexibleColumnLayout",
				"transition": "slide",
				"bypassed": {
					"target": "home"
				},
				"async": true
			},
			"routes": [
				{
					"name": "home",
					"pattern": "",
					"target": "home"
				},
				{
					"pattern": "master",
					"name": "master",
					"target": "master"
				},
				{
					"pattern": "master2",
					"name": "master2",
					"target": "master2"
				},
				{
					"pattern": "master3",
					"name": "master3",
					"target": "master3"
				},
				{
					"pattern": "detail/{product}/{layout}",
					"name": "detail",
					"target": [
						"master",
						"detail"
					]
				},
				{
					"pattern": "detailMaster3/{product}/{layout}",
					"name": "detailMaster3",
					"target": [
						"master3",
						"detailMaster3"
					]
				},
				{
					"pattern": "Detail2Master3/{product}/{layout}",
					"name": "Detail2Master3",
					"target": [
						"detailMaster3",
						"Detail2Master3"
					]
				},
				{
					"pattern": "prova/{product}/{layout}",
					"name": "prova",
					"target": [
						"master3",
						"prova"
					]
				}
				
			],
			"targets": {
				"home": {
					"viewName": "Home",
					"viewLevel": 1,
					"controlAggregation": "beginColumnPages"
				},
				"master": {
					"viewName": "Master",
					"controlAggregation": "beginColumnPages",
					"viewLevel": 2
				},
				"master2": {
					"viewName": "Master2",
					"controlAggregation": "beginColumnPages",
					"viewLevel": 2
				},
				"master3": {
					"viewName": "Master3",
					"controlAggregation": "beginColumnPages",
					"viewLevel": 2
				},
				"detail": {
					"viewName": "Detail",
					"controlAggregation": "midColumnPages",
					"viewLevel": 2
				},
				"detailMaster3": {
					"viewName": "DetailMaster3",
					"controlAggregation": "beginColumnPages",
					"viewLevel": 2
				},
				"Detail2Master3": {
					"viewName": "Detail2Master3",
					"controlAggregation": "midColumnPages",
					"viewLevel": 2
				},
				"prova": {
					"viewName": "Detail2Master3",
					"controlAggregation": "midColumnPages",
					"viewLevel": 2
				}


			}
		}
	}
}