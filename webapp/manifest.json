{
	"_version": "1.66.0",
	"sap.app": {
		"id": "programmi.consegne.edi",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainServiceV4": {
				"uri": "/odata/v4/delivery-forecast/",
				"type": "OData",
				"settings": {
				"annotations": [],
				"odataVersion": "4.0"
				}
			},
			"mainServiceV2": {
				"uri": "/odata/v2/delivery-forecast/",
				"type": "OData",
				"settings": {
				"annotations": [],
				"odataVersion": "2.0"
				}
			},
			"calloffService": {
				"uri": "/odata/v4/calloff/",
				"type": "OData",
				"settings": {
				"annotations": [],
				"odataVersion": "4.0"
				}
			},
			"calloffService2": {
				"uri": "/odata/v2/calloff/",
				"type": "OData",
				"settings": {
				"annotations": [],
				"odataVersion": "2.0"
				}
			},
			"selfBillingService": {
				"uri": "/odata/v4/self-billing/",
				"type": "OData",
				"settings": {
				"annotations": [],
				"odataVersion": "4.0"
				}
			},
			"selfBillingService2": {
				"uri": "/odata/v2/self-billing/",
				"type": "OData",
				"settings": {
				"annotations": [],
				"odataVersion": "2.0"
				}
			},
      "fileScartatiService": {
				"uri": "/odata/v4/file-scartati/",
				"type": "OData",
				"settings": {
				"annotations": [],
				"odataVersion": "4.0"
				}
			},
			"fileScartatiService2": {
				"uri": "/odata/v2/file-scartati/",
				"type": "OData",
				"settings": {
				"annotations": [],
				"odataVersion": "2.0"
				}
			}
	}
	},
	"sap.ui5": {
		"flexEnabled": false,
    "contentDensities": {"compact": true, "cozy": true},
		"rootView": {
			"viewName": "programmi.consegne.edi.view.App",
			"type": "XML",
			"async": true,
			"id": "fcl"
		},
		"dependencies": {
            "minUI5Version": "1.132.1",
            "libs": {
                "sap.f": {},
                "sap.m": {},
                "sap.ui.core": {},
                "sap.uxap": {},
                "sap.viz": {}

            }
        },
		"models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "cruscottopassivo.i18n.i18n"
        }
      },
      "modelloV4": {
        "dataSource": "mainServiceV4",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "modelloV2": {
        "dataSource": "mainServiceV2",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
	  "calloffV4": {
        "dataSource": "calloffService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "calloffV2": {
        "dataSource": "calloffService2",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
	  "selfBillingV4": {
        "dataSource": "selfBillingService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "selfBillingV2": {
        "dataSource": "selfBillingService2",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
	    },
      "fileScartatiV4": {
        "dataSource": "fileScartatiService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "fileScartatiV2": {
        "dataSource": "fileScartatiService2",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
	    }
    },
		"resources": {
			"css": [
			  {
				"uri": "css/style.css"
			  }
			],
      "img": [
        {
          "uri": "public/images/",
          "name": "borghi.images"
        }
      ]
		},
		"config": {
			"fullWidth": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "programmi.consegne.edi.view",
				"controlId": "flexibleColumnLayout",
				"transition": "slide",
				"bypassed": {
					"target": "home"
				},
				"async": true
			},
			"routes": [
				{
					"name": "home",
					"pattern": "",
					"target": "home"
				},
				{
					"pattern": "master/:monitor:",
					"name": "master",
					"target": "master"
				},
				{
					"pattern": "master2/:monitor:",
					"name": "master2",
					"target": "master2"
				},
				{
					"pattern": "master3",
					"name": "master3",
					"target": "master3"
				},       
				{
					"pattern": "detail/{product}/{layout}",
					"name": "detail",
					"target": [
						"master",
						"detail"
					]
				},
				{
					"pattern": "detailMaster3/{product}/{layout}",
					"name": "detailMaster3",
					"target": [
						
						"detailMaster3"
					]
				},
				{
					"pattern": "Detail2Master3/{product}/{layout}",
					"name": "Detail2Master3",
					"target": [
            
						"Detail2Master3"
					]
				},
				{
					"pattern": "archivio",
					"name": "archivio",
					"target": "archivio"
				},
        {
					"pattern": "dettCallOff/{id}",
					"name": "dettCallOff",
					"target": [
						
						"dettCallOff"
					]
        },
        {
					"pattern": "dettSelfBilling/{id}",
					"name": "dettSelfBilling",
					"target": [
            
            "dettSelfBilling"
          ]
        }

			],
			"targets": {
				"home": {
					"viewName": "Home",
					"viewLevel": 1,
					"controlAggregation": "beginColumnPages"
				},
				"master": {
					"viewName": "Master",
					"controlAggregation": "beginColumnPages",
					"viewLevel": 2
				},
				"master2": {
					"viewName": "Master2",
					"controlAggregation": "beginColumnPages",
					"viewLevel": 2
				},
				"master3": {
					"viewName": "Master3",
					"controlAggregation": "beginColumnPages",
					"viewLevel": 2
				},
				"detail": {
					"viewName": "Detail",
					"controlAggregation": "midColumnPages",
					"viewLevel": 2
				},
				"detailMaster3": {
					"viewName": "DetailMaster3",
					"controlAggregation": "beginColumnPages",
					"viewLevel": 2,
					"viewId": "idDetailMaster3"
				},
				"Detail2Master3": {
					"viewName": "Detail2Master3",
					"controlAggregation": "midColumnPages",
					"viewLevel": 2
				},
				"archivio": {
					"viewName": "archivio",
					"controlAggregation": "beginColumnPages",
					"viewLevel": 2
				},
        "dettCallOff": {
					"viewName": "dettCallOff",
					"controlAggregation": "beginColumnPages",
					"viewLevel": 2,
          "viewId": "idDetailCallOff"
				},
        "dettSelfBilling": {
          "viewName": "dettSelfBilling",
          "controlAggregation": "beginColumnPages",
					"viewLevel": 2,
          "viewId": "idDetailSelfBilling"
				}

			}
		}
	}
}