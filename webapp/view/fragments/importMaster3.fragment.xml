<core:FragmentDefinition xmlns:unified="sap.ui.unified" xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog title="Caricamento file Excel">
        <content>
            <VBox class= "sapUiMediumMargin">
                
                <HBox class="sapUiMediumMarginBottom">
                    <unified:FileUploader ></unified:FileUploader>
                    <Button type="Emphasized" icon="sap-icon://upload" text="Upload" class="sapUiSmallMarginBegin"></Button>
                </HBox>
                
            </VBox>
        </content>
        <buttons>
          <Button type="Emphasized" icon="sap-icon://download" text="Scarica template"/>
          
            <Button icon="sap-icon://decline"  text="Chiudi" press="onclose"/> 
        </buttons>
    </Dialog>
</core:FragmentDefinition>