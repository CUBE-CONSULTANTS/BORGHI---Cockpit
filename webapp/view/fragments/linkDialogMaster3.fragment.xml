<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog title="Fine del processamento">
        <content>
            <VBox class= "sapUiMediumMargin">
                <HBox class="sapUiLargeMarginBottom" >
                    <Text text="Processo terminato senza errori"/>
                </HBox>
                <HBox >
                    <Link text="Reportistica Cumulativi" press="onLink1Press" class="sapUiLargeMarginEnd"/>
                    <Link text="Reportistica Progressivi" press="onLink2Press"/>
                </HBox>
            </VBox>
        </content>
        <buttons>
            <Button text="Chiudi" press="onclose"/> 
        </buttons>
    </Dialog>
</core:FragmentDefinition>