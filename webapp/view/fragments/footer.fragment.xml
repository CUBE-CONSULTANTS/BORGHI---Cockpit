<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
<OverflowToolbar>
          
  <Button
      press="navToHome"
      icon="sap-icon://home"
      text="Home"
      type="Emphasized"
  />

  <ToolbarSpacer />
  <Button
      press="navToMonitor"
      icon="sap-icon://navigation-right-arrow"
      text="Monitor"
      type="Emphasized"
      visible="{= ${main>/backToMon}}"
  />
  <Button
      press="navToArchive"
      icon="sap-icon://navigation-right-arrow"
      text="Archivio"
      type="Emphasized"
      visible="{= ${main>/backToArch}}"
  />
</OverflowToolbar>
</c:FragmentDefinition>