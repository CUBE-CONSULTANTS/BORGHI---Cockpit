<core:FragmentDefinition   xmlns="sap.m"
xmlns:core="sap.ui.core"
xmlns:f="sap.f"
xmlns:fb="sap.ui.comp.filterbar"
xmlns:ui="sap.ui.table"
xmlns:mvc="sap.ui.core.mvc">

   
            <fb:FilterBar
            id="rthr"
            persistencyKey="myPersKey"
            useToolbar="false"
            search=".onSearch"
            filterChange=".onFilterChange"
            afterVariantLoad=".onAfterVariantLoad"
            showGoOnFB="true"
            showFilterConfiguration="true"
            class="sapUiSmallMarginBottom"
        >
            <fb:filterGroupItems>
                <fb:FilterGroupItem
                    name="Data Ricezione EDI"
                    label="Data Ricezione EDI"
                    groupName="Group1"
                    visibleInFilterBar="true"
                >
                    <fb:control>
                    <DatePicker
                    tooltip="..."
                    secondaryCalendarType="Gregorian"
                    change="handleChange"
                    />
                    </fb:control>
                </fb:FilterGroupItem>
                
                <fb:FilterGroupItem
                    name="Cliente"
                    label="Cliente"
                    groupName="Group1"
                    visibleInFilterBar="true"
                >
                    <fb:control>
                       <Input/>
                    </fb:control>
                </fb:FilterGroupItem>
                 <fb:FilterGroupItem
                    name="Materiale"
                    label="Materiale"
                    groupName="Group1"
                    visibleInFilterBar="true"
                >
                    <fb:control>
                    <Input/>
                    </fb:control>
                </fb:FilterGroupItem>
                
                 <fb:FilterGroupItem
                    name="Reason"
                    label="Reason"
                    groupName="Group1"
                    visibleInFilterBar="true"
                >
                    <fb:control>
                       <Input/>
                    </fb:control>
                </fb:FilterGroupItem>
                
            </fb:filterGroupItems>
        </fb:FilterBar>
        




        <ui:Table
				rows="{master3>/}"
				selectionMode="MultiToggle"
				paste="onPaste"
				ariaLabelledBy="title">
				<ui:extension>
					<OverflowToolbar style="Clear">
						
					</OverflowToolbar>
				</ui:extension>
				<ui:columns>
					<ui:Column width="11rem">
						<Label text="Progressivo trasmissione precedente" />
						<ui:template>
							<Text text="{master3>progressivo_ultimo_invio}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Progressivo trasmissione" />
						<ui:template>
							<Text text="{master3>codice_articolo}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Data trasmissione" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="9rem">
						<Label text="Codice DDT" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="9rem">
						<Label text="Data di spedizione" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Punto di scarico" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="6rem">
						<Label text="Tipo di invio" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="14rem">
						<Label text="Numero cliente" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Numero contratto" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Codice processo" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Stabilimento cliente" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Consigment reference" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="ID del ricevente" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Magazzino cliente" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="ID fornitore" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Punto di consumo" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Riferimento cliente" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Quantità spedita" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Unità di misura" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						<Label text="Consegna originale" />
						<ui:template>
							<Text text="{Name}" wrapping="false" />
						</ui:template>
					</ui:Column>
          <ui:Column width="11rem">
						
						<ui:template>
							<Button
                  tooltip="Stock Level"
                  icon="sap-icon://doc-attachment"
                  />
						</ui:template>
					</ui:Column>
				
				</ui:columns>
			</ui:Table>
   
</core:FragmentDefinition>